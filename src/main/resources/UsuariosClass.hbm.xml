<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.example.DTO.UsuariosClass" table="usuarios" schema="public" catalog="postgres">
        <id name="idUsuario">
            <column name="id_usuario" sql-type="integer"/>
        </id>
        <property name="nombreUsuario">
            <column name="nombre_usuario" sql-type="varchar(255)"/>
        </property>
        <property name="contraseña">
            <column name="contraseña" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>